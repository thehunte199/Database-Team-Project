CREATE TABLE member (
	bannerID char(9) NOT NULL,
	memFName varchar(25) NOT NULL,
	memMName varchar(25),
	memLName varchar(25) NOT NULL,
	memEmail varchar(50) NOT NULL,
	memPhoneNum char(10),
	memCollege varchar(50),
	memMajor varchar(30),
	memClass varchar(8),
	shirtSize varchar(3),
	membershipStatus bit(),

	CONSTRAINT PRIMARY KEY (bannerID),
);
